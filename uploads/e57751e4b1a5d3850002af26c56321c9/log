  142 /////////// Transform from point P0 to start of magnet BB with given centre //////////
  143 OFFSET_X(delta_x, delta_y, tilt, bb_length, x_out): MACRO {
  144     x_out = delta_x-bb_length*cos(tilt)/2.;
  145 }
  146 
  147 OFFSET_Y(delta_x, delta_y, tilt, bb_length, y_out): MACRO {
  148     y_out = delta_y-bb_length*sin(tilt)/2.;
  149 }
  150 
  151 NORM_X(delta_x, delta_y, tilt, bb_length, tx_out): MACRO {
  152     tx_out = cos(tilt)/2.;
  153 }
  154 
  155 NORM_Y(delta_x, delta_y, tilt, bb_length, ty_out): MACRO {
  156     ty_out = sin(tilt)/2.;
  157 }
  158 
  159 MAGNET_OFFSET(delta_x, delta_y, tilt, bb_length, x_out, y_out, tx_out, ty_out): MACRO {
  160     OFFSET_X(delta_x, delta_y, tilt, bb_length, x_out);
  161     OFFSET_Y(delta_x, delta_y, tilt, bb_length, y_out);
  162     NORM_X  (delta_x, delta_y, tilt, bb_length, tx_out);
  163     NORM_Y  (delta_x, delta_y, tilt, bb_length, ty_out);
  164 }
  165 
  166 // cartesian offset, including a bounding box
  167 BUILD_CENTRE_OFFSET(name, delta_x, delta_y, tilt, bb_length): MACRO {
  168     REAL x_out=0;
  169     REAL y_out=0;
  170     REAL tx_out=0;
  171     REAL ty_out=0;
  172 
  173     MAGNET_OFFSET(delta_x, delta_y, tilt, bb_length, x_out, y_out, tx_out, ty_out);
  174 
  175     name: LOCAL_CARTESIAN_OFFSET,
  176             end_position_x=x_out,
  177             end_position_y=y_out,
  178             end_normal_x=tx_out,
  179             end_normal_y=ty_out;
  180 }
  181 
  182 
  183 // place an element on the circle having radius R at azimuthal angle phi
  184 // offset from the circle by delta r and then tilt by delta phi
  185 BUILD_AZIMUTHAL_OFFSET(name, radius, phi, delta_r, delta_phi, bb_length): MACRO {
  186     REAL x_out=0;
  187     REAL y_out=0;
  188     REAL tx_out=0;
  189     REAL ty_out=0;
  190     //REAL cell_phi=2*PI/N_CELLS/2;
  191 
  192     // delta_x/y in the coordinate system of the ring
  193     REAL delta_x = radius*sin(phi)+delta_r*sin(phi);
  194     REAL delta_y = -radius*(1-cos(phi))+delta_r*cos(phi);
  195 
  196     MAGNET_OFFSET(delta_x, delta_y, -(phi+delta_phi), bb_length, x_out, y_out, tx_out, ty_out);
  197 
  198     name: LOCAL_CARTESIAN_OFFSET,
  199             end_position_x=x_out,
  200             end_position_y=y_out,
  201             end_normal_x=tx_out,
  202             end_normal_y=ty_out;
  203 }
  204 
  205 
  206 /////////// Transform from end of magnet BB back to the point P0 //////////
  207 
  208 R_OFFSET_X(delta_x, delta_y, tilt, bb_length, x_out): MACRO {
  209     x_out =-delta_y*sin(tilt)-delta_x*cos(tilt)-bb_length/2.;
  210 }
  211 
  212 R_OFFSET_Y(delta_x, delta_y, tilt, bb_length, y_out): MACRO {
  213     y_out = -delta_y*cos(tilt)+delta_x*sin(tilt);
  214 }
  215 
  216 R_NORM_X(delta_x, delta_y, tilt, bb_length, tx_out): MACRO {
  217     tx_out = cos(tilt)/2.;
  218 }
  219 
  220 R_NORM_Y(delta_x, delta_y, tilt, bb_length, ty_out): MACRO {
  221     ty_out = -sin(tilt)/2.
  222 }
  223 
  224 REVERSE_MAGNET_OFFSET(delta_x, delta_y, tilt, bb_length, x_out, y_out, tx_out, ty_out): MACRO {
  225     R_OFFSET_X(delta_x, delta_y, tilt, bb_length, x_out);
  226     R_OFFSET_Y(delta_x, delta_y, tilt, bb_length, y_out);
  227     R_NORM_X  (delta_x, delta_y, tilt, bb_length, tx_out);
  228     R_NORM_Y  (delta_x, delta_y, tilt, bb_length, ty_out);
  229 }
  230 
  231 // cartesian offset, including a bounding box
  232 // undoes a placement like BUILD_CENTRE_OFFSET; rectangular element
  233 BUILD_CENTRE_REVERSE_OFFSET(name, delta_x, delta_y, tilt, bb_length): MACRO {
  234     REAL x_out=0;
  235     REAL y_out=0;
  236     REAL tx_out=0;
  237     REAL ty_out=0;
  238 
  239     REVERSE_MAGNET_OFFSET(delta_x, delta_y, tilt, bb_length, x_out, y_out, tx_out, ty_out);
  240 
  241     name: LOCAL_CARTESIAN_OFFSET,
  242             end_position_x=x_out,
  243             end_position_y=y_out,
  244             end_normal_x=tx_out,
  245             end_normal_y=ty_out;
  246 }
  247 
  248 // place an element on the circle having radius R at azimuthal angle phi
  249 // offset from the circle by delta r and then tilt by delta phi
  250 BUILD_AZIMUTHAL_REVERSE_OFFSET(name, radius, phi, delta_r, delta_phi, bb_length): MACRO {
  251     REAL x_out=0;
  252     REAL y_out=0;
  253     REAL tx_out=0;
  254     REAL ty_out=0;
  255     //REAL cell_phi=2*PI/N_CELLS/2;
  256 
  257     // delta_x/y in the coordinate system of the ring
  258     REAL delta_x = radius*sin(phi)+delta_r*sin(phi);
  259     REAL delta_y = -radius*(1-cos(phi))+delta_r*cos(phi);
  260 
  261     REVERSE_MAGNET_OFFSET(delta_x, delta_y, -(phi+delta_phi), bb_length, x_out, y_out, tx_out, ty_out);
  262 
  263     name: LOCAL_CARTESIAN_OFFSET,
  264             end_position_x=x_out,
  265             end_position_y=y_out,
  266             end_normal_x=tx_out,
  267             end_normal_y=ty_out;
  268 }
